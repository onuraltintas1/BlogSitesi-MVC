@model System.Collections.Generic.List<EntityLayer.Concrete.Category>

<div class="tech-btm">
    <h4>Kategoriler</h4>
    @foreach (var item in Model)
    {
        <ul class="list-group single">
            <li class="list-group-item d-flex justify-content-between align-items-center">
                @item.CategoryName
                <span class="badge badge-primary badge-pill">@(item.Blogs != null ? item.Id.ToString().Length : 0)</span>
            </li>

        </ul>
    }
</div>